<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Admin page</title>

    <link rel="icon" type="image/x-icon" href="~/images/favicon.ico" />
    <link href="~/css/bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <link href="~/css/layouts/modern-light-menu/light/loader.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layouts/modern-light-menu/dark/loader.css" rel="stylesheet" type="text/css" />

    <link href="~/css/layouts/modern-light-menu/light/structure.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layouts/modern-light-menu/dark/structure.css" rel="stylesheet" type="text/css" />

    <script src="~/js/layouts/modern-light-menu/loader.js"></script>

    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Nunito:400,600,700" rel="stylesheet">

    <link href="~/css/bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <link href="~/css/layouts/modern-light-menu/light/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layouts/modern-light-menu/dark/plugins.css" rel="stylesheet" type="text/css" />

    <link href="~/css/assets/dark/main.css" rel="stylesheet" type="text/css" />
    <link href="~/css/assets/light/main.css" rel="stylesheet" type="text/css" />

    <link href="~/css/assets/dark/components/modal.css" rel="stylesheet" type="text/css" />
    <link href="~/css/assets/light/components/modal.css" rel="stylesheet" type="text/css" />

    <link href="~/lib/plugins/highlight/styles/monokai-sublime.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/plugins/notification/snackbar/snackbar.min.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/plugins/waves/waves.min.css" rel="stylesheet" type="text/css" />

    <link href="~/css/assets/dark/scrollspyNav.css" rel="stylesheet" type="text/css" />
    <link href="~/css/assets/light/scrollspyNav.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM STYLES -->
    <link href="~/lib/plugins/apex/apexcharts.css" rel="stylesheet" type="text/css">
    <link href="~/css/assets/light/dashboard/dash_1.css" rel="stylesheet" type="text/css" />
    <link href="~/css/assets/dark/dashboard/dash_1.css" rel="stylesheet" type="text/css" />

    <link href="~/lib/plugins/light/sweetalerts2/custom-sweetalert.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/plugins/dark/sweetalerts2/custom-sweetalert.css" rel="stylesheet" type="text/css" />

    <!-- END PAGE LEVEL PLUGINS/CUSTOM STYLES -->
</head>
<body class="layout-boxed">

    <!-- BEGIN LOADER -->
    <div id="load_screen">
        <div class="loader">
            <div class="loader-content">
                <div class="spinner-grow align-self-center"></div>
            </div>
        </div>
    </div>
    <!--  END LOADER -->

    <!--  BEGIN NAVBAR  -->
    @Html.Partial("~/Views/Shared/Navbar/_StyleVerticalMenu.cshtml")
    <!--  END NAVBAR  -->

    <!--  BEGIN MAIN CONTAINER  -->
    <div class="main-container" id="container">

        <div class="overlay"></div>
        <div class="search-overlay"></div>

        <!--  BEGIN SIDEBAR  -->
        @Html.Partial("~/Views/Shared/Menu/_VerticalMenu.cshtml")
        <!--  END SIDEBAR  -->

        <!--  BEGIN CONTENT AREA  -->
        <div id="content" class="main-content">
            @RenderBody()

            <!--  BEGIN FOOTER  -->
            <div class="footer-wrapper">
                <div class="footer-section f-section-1">
                    <p class="">
                        Copyright © <span class="dynamic-year">2022</span> 
                        <a target="_blank" href="https://designreset.com/cork-admin/">DesignReset</a>, 
                        All rights reserved.
                    </p>
                </div>
                <div class="footer-section f-section-2">
                    <p class="">
                        Coded with
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                             viewBox="0 0 24 24" fill="none" stroke="currentColor"
                             stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                             class="feather feather-heart">
                            <path d="M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z"></path>
                        </svg>
                    </p>
                </div>
            </div>
            <!--  END FOOTER  -->
        </div>
        <!--  END CONTENT AREA  -->

    </div>
    <!-- END MAIN CONTAINER -->

    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/js/assets/scrollspyNav.js"></script>
    <script src="~/lib/plugins/global/vendors.min.js"></script>
    <script src="~/lib/plugins/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/lib/plugins/mousetrap/mousetrap.min.js"></script>
    <script src="~/lib/plugins/waves/waves.min.js"></script>
    <script src="~/lib/plugins/notification/snackbar/snackbar.min.js"></script>
    <script src="~/lib/plugins/font-icons/feather/feather.min.js"></script>
    <script src="~/js/layouts/modern-light-menu/app.js"></script>
    <!-- END GLOBAL MANDATORY SCRIPTS -->

    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->
    <script src="~/lib/plugins/apex/apexcharts.min.js"></script>
    <script src="~/js/assets/dashboard/dash_1.js"></script>
    <script src="~/lib/plugins/sweetalerts2/sweetalerts2.min.js"></script>
    <!-- BEGIN PAGE LEVEL PLUGINS/CUSTOM SCRIPTS -->

    <script src="~/js/assets/custom.js"></script>
    <script src="~/js/assets/elements/custom-search.js"></script>
    <script src="~/js/app.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
